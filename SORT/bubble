"""冒泡排序Python实现"""

def bubble_sort(lst):
    for x in range(0, len(lst)):
        for y in range(x+1, len(lst)):
            if lst[x] > lst[y]:
                lst[x], lst[y] = lst[y], lst[x]
    return a_1


if __name__ == '__main__':
    a_1 = [1, 5, 9, 10, 89, 45, 86, 57]
    a_2 = bubble_sort(a_1)
    [print(each, end=" ") for each in a_2]

